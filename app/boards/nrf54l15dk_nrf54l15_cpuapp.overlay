
&uart21 {
    status = "okay";
    pinctrl-0 = <&uart21_default>;
    pinctrl-names = "default";
    current-speed = <9600>;
    m90e26: m90e26@0 {
        compatible = "atmel,m90e26";
        reg = <0 0 1>;
    };
};

&pinctrl {
    uart21_default: uart21_default {
        group1 {
            psels = <NRF_PSEL(UART_TX, 1, 0)>, <NRF_PSEL(UART_RX, 1, 1)>;
        };
    };
};